<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.direct.entity.DepartmentInfo" table="departmentinfo">
		<id name="deptid" column="deptid">
			<generator class="identity" />
		</id>
		<property name="deptName" column="deptName" />
		<property name="principal" column="principal" />
		<property name="deptPhone" column="deptPhone" />
		<property name="deptFax" column="deptFax" />
		<property name="remark" column="remark" />
	</class>
</hibernate-mapping>